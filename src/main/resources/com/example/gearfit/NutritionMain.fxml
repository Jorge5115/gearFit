<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="850.0" styleClass="parent" stylesheets="@styles/NutritionMain.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gearfit.controllers.NutritionMainController">
    <children>
        <Pane layoutX="13.0" layoutY="13.0" prefHeight="475.0" prefWidth="825.0" styleClass="pane">
         <children>
            <VBox alignment="CENTER" layoutX="566.0" layoutY="145.0">
               <children>
                  <VBox fx:id="vboxFoodRegisteredList" alignment="TOP_CENTER" prefHeight="250.0" />
                  <Button mnemonicParsing="false" onAction="#handleAddNewFood" styleClass="add-new-food-button" text="Añadir Nuevo Alimento" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" layoutX="283.0" layoutY="145.0" prefHeight="295.0" prefWidth="260.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleAddBreakfast" styleClass="add-breakfast-button" text="Añadir Desayuno" />
                  <VBox fx:id="vboxBreakfastList" prefHeight="125.0" prefWidth="260.0" />
                  <Button layoutX="87.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleAddLunch" styleClass="add-lunch-button" text="Añadir Comida" />
                  <VBox fx:id="vboxLunchList" layoutX="10.0" layoutY="35.0" prefHeight="125.0" prefWidth="260.0" />
               </children>
            </VBox>
            <HBox fx:id="weeklyDaysHbox" layoutX="24.0" layoutY="14.0" prefHeight="100.0" prefWidth="777.0" style="-fx-background-color: #e21342;" styleClass="hbox-weekly-days" />
         </children></Pane>
    </children>
</AnchorPane>
